<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>loginController</servlet-name>
        <servlet-class>Controller.loginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutController</servlet-name>
        <servlet-class>Controller.logoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Controller.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>makeQuiz</servlet-name>
        <servlet-class>Controller.makeQuiz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>takeQuiz</servlet-name>
        <servlet-class>Controller.takeQuiz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processTakeQuiz</servlet-name>
        <servlet-class>Controller.processTakeQuiz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeProcess</servlet-name>
        <servlet-class>Controller.TimeProcess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginController</servlet-name>
        <url-pattern>/loginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutController</servlet-name>
        <url-pattern>/logoutController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>makeQuiz</servlet-name>
        <url-pattern>/makeQuiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>takeQuiz</servlet-name>
        <url-pattern>/takeQuiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processTakeQuiz</servlet-name>
        <url-pattern>/processTakeQuiz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimeProcess</servlet-name>
        <url-pattern>/TimeProcess</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <error-code>404</error-code>
        <location>/error404.jsp</location>
    </error-page>
</web-app>
